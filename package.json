{
  "name": "learnreact",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "migrate": "nx migrate latest",
    "migrate:run": "nx migrate --run-migrations",
    "scrimba:start": "nx run scrimba:serve",
    "scrimba:demo": "nx run scrimba:build && lite-server -c bs-config-scrimba.js",
    "build": "nx affected --target=build --parallel --max-parallel=3",
    "test": "nx affected --target=test --parallel --max-parallel=3",
    "lint": "nx affected --target=lint --fix --parallel --max-parallel=3",
    "ci": "npm run lint && npm run test && npm run build",
    "e2e:scrimba": "nx e2e scrimba-e2e --watch",
    "deploy:prod-draft": "nx affected --target=deploy-prod-draft",
    "deploy:prod": "nx affected --target=deploy-prod",
    "format": "nx format:write --all",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "core-js": "3.6.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@namics/commitlint-conventional-changelog": "1.2.2",
    "@nrwl/cli": "14.7.5",
    "@nrwl/cypress": "14.7.5",
    "@nrwl/eslint-plugin-nx": "14.7.5",
    "@nrwl/jest": "14.7.5",
    "@nrwl/linter": "14.7.5",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "14.7.5",
    "@nrwl/web": "14.7.5",
    "@nrwl/workspace": "14.7.5",
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "babel-jest": "28.1.1",
    "cypress": "10.7.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lite-server": "2.6.1",
    "netlify-cli": "11.7.1",
    "nx": "14.7.5",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  }
}
